{
  "name": "eform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start -o",
    "build": "react-scripts build",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "build:dev": "env-cmd -f .env.dev npm run build && firebase deploy --only hosting:eform-1",
    "start:staging:tekmarkgroup": "env-cmd -f .env.staging-tekmark react-scripts start",
    "build:staging:tekmarkgroup": "env-cmd -f .env.staging-tekmark npm run build && firebase deploy --only hosting:staging-tekmarkgroup",
    "start:prod:tekmarkgroup": "env-cmd -f .env.production-tekmark react-scripts start",
    "build:prod:tekmarkgroup": "env-cmd -f .env.production-tekmark npm run build && firebase deploy --only hosting:tekmarkgroup",
    "start:prod:dotcom": "env-cmd -f .env.production-dotcom react-scripts start",
    "build:prod:dotcom": "env-cmd -f .env.production-dotcom npm run build && firebase deploy --only hosting:dotcom-net",
    "start:prod:intrailmu": "env-cmd -f .env.production-intrailmu react-scripts start",
    "build:prod:intrailmu": "env-cmd -f .env.production-intrailmu npm run build && firebase deploy --only hosting:intrailmu",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
    "rtl": "webpack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
